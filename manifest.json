{
  "manifest_version": 3,
  "name": "AIバトラービューア",
  "version": "1.0",
  "description": "AIバトラーの対戦ログを一括で見るぞおお！",
  "content_security_policy": {
    "extension_pages": "default-src 'self'; connect-src https://ai-battle.alphabrend.com/; script-src 'self'"
  },
  "icons": {
    "48": "gotsuo.png"
  },
  "permissions": [
    "cookies",
    "webNavigation",
    "webRequest"
  ],
  "action": {
    "default_icon": "gotsuo.png",
    "default_popup": "popup.html"
  },
  "author": "fumieval",
  "homepage_url": "https://github.com/fumieval/ai-battler-viewer"
}
